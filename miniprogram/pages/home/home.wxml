<view class="page-home bg-f53f3b">
  <loding wx:if="{{ isLoding }}">

  </loding>
  <view class="box-page_home warp ptb-30">
    <view class="header flex-sa bg-ffffff p-20">
      <view class="item">
        <view class="flex-cen">
          <image src="../../images/icon-sign.png" class="item-icon ptb-10" mode="" />
        </view>
        <view class="fs-30">
          每日签到
        </view>
      </view>
      <view class="item">
        <view class="flex-cen">
          <image src="../../images/icon-event.png" class="item-icon ptb-10" mode="" />
        </view>
        <view class="fs-30">
          参加活动
        </view>
      </view>
      <view class="item">
        <view class="flex-cen">
          <image src="../../images/icon-business.png" class="item-icon ptb-10" mode="" />
        </view>
        <view class="fs-30">
          商家入驻
        </view>
      </view>
      <view class="item">
        <view class="flex-cen">
          <image src="../../images/icon-event.png" class="item-icon ptb-10" mode="" />
        </view>
        <view class="fs-30">
          积分
        </view>
      </view>
    </view>
    <view class="content ptb-20">
      <!-- <tabs tabsList="{{tabsList}}" class="tab" bindtabsItemChange="getTabsItemChange">
      </tabs> -->
      <tabs rewrite-tabs_title="rewrite-tabs_title" bor-radius="bor-radius" tabsList="{{tabsList}}" class="tabs" bindtabsItemChange="getTabsItemChange">
        <view slot="0">
          <view class="activity-warp warp " wx:if="{{isLoaded}}">
            <view class="flex-column activity-all  ">
              <activity class="activity-item mt-20" wx:for="{{tabListData}}" wx:key="index" activityInfo="{{item}}" bindtap="navigateto" data-to="{{item}}">
                <view>
                </view>
              </activity>
            </view>
            <view class="fs-30 c-919191 flex-c p-30">
              我也是有底线哒
            </view>
          </view>

          <view class="flex-column ptb-100" wx:if="{{!activityList}}">
            <view class="flex-c mt-80">
              <image class="icon-none" src="../../images/icon-none.png" mode="" />
            </view>
            <view class="flex-c mt-60 c-919191">
              空空如也~~
            </view>
          </view>
        </view>
      </tabs>
      <view>

      </view>
    </view>
  </view>
</view>

