<view class="review-merchant-detail-box">
  <view class="form">
    <view class="item">
      <view class="fw-600 item-title">店铺名称</view>
      <view>
        {{ businessDetail.storeName }}
      </view>
    </view>
    <view class="item">
      <view class="fw-600 item-title">联系方式</view>
      <view>
        {{ businessDetail.phone }}
      </view>
    </view>
    <view class="item">
      <view class="fw-600 item-title">负责人</view>
      <view>
        {{ businessDetail.name }}
      </view>
    </view>
    <view class="item" bindtap="upImage">
      <view class="item-title item-title">
        营业执照
      </view>

      <view class="printing">
        <image src="{{ businessDetail.printingLicenceUrl }}" mode="" />
      </view>

    </view>

    <view class="item">
      <view class="fw-600 item-title">店铺地址</view>
      <view>
        <picker mode="region" bindchange="bindRegionChange">
          <view wx:if="{{businessDetail.address.length}}" class="address">
            <text>{{ businessDetail.address[0] }}-{{ businessDetail.address[1] }}-{{ businessDetail.address[2] }}</text>
            <text class="pl-20">{{ businessDetail.detailedAddress }}</text>
          </view>
        </picker>
      </view>
    </view>
  </view>
  <view class="flex-around">
    <view class="approved" bindtap="dialogReviewOkFn">
      审核通过
    </view>
    <view class="no-approved" bindtap="dialogReviewNoOkFn">
      未审核通过
    </view>
  </view>
</view>